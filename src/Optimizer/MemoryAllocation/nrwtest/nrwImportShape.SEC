# Importing NRW Data

close database

create database nrw2

open database nrw2

let Roads = dbimport2('../bin/nrw/roads.dbf') addcounter[No, 1] shpimport2('../bin/nrw/roads.shp') namedtransformstream[GeoData] addcounter[No2, 1] mergejoin[No, No2] remove[No, No2] filter[isdefined(bbox(.GeoData))] consume

