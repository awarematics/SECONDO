#testcase count2
#yields (int 10)
query ten feed count2;


#testcase Filter1
#yields (int 5)
query ten feed filter[.No > 5] consume count;

#testcase Filter2
#yields (int 0)
query ten feed head[0] filter[.No > 5] consume count;

#testcase Filter3
#yields error
