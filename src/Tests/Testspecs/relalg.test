(create empty : (rel(tuple((No int)))));
(update empty := ((rel(tuple((No int))))()));

(create two : (rel(tuple((No int)))));
(update two := ((rel(tuple((No int))))((1)(2))));

(create three : (rel(tuple((No int)))));
