(
  (1 ())
  (2 ((0 0)))
  (3 ((1 1)))
  (4 ((0 0) (0 1) (1 0) (1 1)))
  (5 ((0 0) (1 0)))
  (6 ((0 1) (1 1)))
  (7 ((0.3 0.5)(0.25 0.75)))
  (8 ((0.3333333333333333 0.5)(0.25 0.7784)(0.5540816326530610 0.4887755102040820)))
